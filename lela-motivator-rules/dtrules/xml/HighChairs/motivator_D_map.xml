	<XMLtoEDD>
		<map>
			<setattribute tag='currentdate' RAttribute='currentdate' enclosure='job' type='date'></setattribute>
			<setattribute tag='motivator' RAttribute='motivator' enclosure='job' type='string'></setattribute>
			<setattribute tag='result' RAttribute='result' enclosure='job' type='integer'></setattribute>
			<setattribute tag='HighChair_ID' RAttribute='HighChair_ID' enclosure='job' type='integer'></setattribute>
			<setattribute tag='Brand_ID' RAttribute='Brand_ID' enclosure='job' type='integer'></setattribute>

			<setattribute tag='HighChair_ID' RAttribute='HighChair_ID' enclosure='highchair' type='string'></setattribute>
			<setattribute tag='Brand_ID' RAttribute='Brand_ID' enclosure='highchair' type='string'></setattribute>
			<setattribute tag='BabbleRecomended' RAttribute='BabbleRecomended' enclosure='highchair' type='string'></setattribute>
			<setattribute tag='JPMAInnovationAwardWinner' RAttribute='JPMAInnovationAwardWinner' enclosure='highchair' type='string'></setattribute>
			<setattribute tag='DesignStyle' RAttribute='DesignStyle' enclosure='highchair' type='string'></setattribute>
