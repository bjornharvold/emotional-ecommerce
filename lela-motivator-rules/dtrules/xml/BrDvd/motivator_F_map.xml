<mapping>
	<XMLtoEDD>
		<map>
			<!--                 -->
			<!-- Map Attributes  -->
			<!--                 -->
			<!--brdvd-->
			<setattribute tag='AudioSystem_THXcertified' RAttribute='AudioSystem_THXcertified' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='Brand' RAttribute='Brand' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='BrandID' RAttribute='BrandID' enclosure='brdvd' type='integer' subtype=''></setattribute>
			<setattribute tag='BrDvdID' RAttribute='BrDvdID' enclosure='brdvd' type='integer' subtype=''></setattribute>
			<setattribute tag='ConsumerReportsBluRayDiscLoadTime' RAttribute='ConsumerReportsBluRayDiscLoadTime' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='ConsumerReportsDVDLoadTime' RAttribute='ConsumerReportsDVDLoadTime' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='ConsumerReportsEaseOfUse' RAttribute='ConsumerReportsEaseOfUse' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='ConsumerReportsPlaybackEaseOfUse' RAttribute='ConsumerReportsPlaybackEaseOfUse' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='ConsumerReportsRecordingEaseOfUse' RAttribute='ConsumerReportsRecordingEaseOfUse' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='DimensionsWeight_Weight' RAttribute='DimensionsWeight_Weight' enclosure='brdvd' type='double' subtype=''></setattribute>
			<setattribute tag='Display_DiagonalSize' RAttribute='Display_DiagonalSize' enclosure='brdvd' type='double' subtype=''></setattribute>
			<setattribute tag='FormFactor' RAttribute='FormFactor' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='motivatorTotal' RAttribute='motivatorTotal' enclosure='brdvd' type='integer' subtype=''></setattribute>
			<setattribute tag='RemoteControl_Technology' RAttribute='RemoteControl_Technology' enclosure='brdvd' type='string' subtype=''></setattribute>
			<!--constants-->
			<!--job-->
			<setattribute tag='BrandID' RAttribute='BrandID' enclosure='job' type='integer' subtype=''></setattribute>
			<setattribute tag='BrDvdID' RAttribute='BrDvdID' enclosure='job' type='integer' subtype=''></setattribute>
			<setattribute tag='currentdate' RAttribute='currentdate' enclosure='job' type='date' subtype=''></setattribute>
			<setattribute tag='motivator' RAttribute='motivator' enclosure='job' type='string' subtype=''></setattribute>
			<setattribute tag='result' RAttribute='result' enclosure='job' type='integer' subtype=''></setattribute>
			<!--                 -->
			<!-- Create Entities -->
			<!--                 -->
			<createentity entity='brdvd' tag='brdvd' id='id'></createentity>
			<createentity entity='constants' tag='constants' id='id'></createentity>
			<createentity entity='job' tag='job' id='id'></createentity>
		</map>
		<!--                 -->
		<!-- Entity List     -->
		<!--                 -->
		<entities>
			<entity name='brdvd' number='*'></entity>
			<entity name='constants' number='1'></entity>
			<entity name='job' number='1'></entity>
		</entities>
		<!--                 -->
		<!-- Initialization  -->
		<!--                 -->
		<initialization>
			<initialentity entity='job' epush='true'></initialentity>
			<initialentity entity='constants' epush='true'></initialentity>
		</initialization>
	</XMLtoEDD>
</mapping>
