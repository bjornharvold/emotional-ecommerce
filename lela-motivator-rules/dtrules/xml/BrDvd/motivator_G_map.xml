			<setattribute tag='BrDvdID' RAttribute='BrDvdID' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='EnvironmentalStandards_ENERGYSTARQualified' RAttribute='EnvironmentalStandards_ENERGYSTARQualified' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='Has3Dglasses' RAttribute='Has3Dglasses' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='ListPrice' RAttribute='ListPrice' enclosure='brdvd' type='double' subtype=''></setattribute>
			<setattribute tag='Miscellaneous_IncludedAccessories' RAttribute='Miscellaneous_IncludedAccessories' enclosure='brdvd' type='string' subtype=''></setattribute>
			<setattribute tag='motivatorTotal' RAttribute='motivatorTotal' enclosure='brdvd' type='integer' subtype=''></setattribute>
			<setattribute tag='motivatorTotalDecimal' RAttribute='motivatorTotalDecimal' enclosure='brdvd' type='double' subtype=''></setattribute>
			<setattribute tag='wireless' RAttribute='wireless' enclosure='brdvd' type='string' subtype=''></setattribute>
