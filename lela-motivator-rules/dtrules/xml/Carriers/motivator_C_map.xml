			<setattribute tag='NAPPAAwardWinner' RAttribute='NAPPAAwardWinner' enclosure='carrier' type='string'></setattribute>
			<setattribute tag='MaximumAge' RAttribute='MaximumAge' enclosure='carrier' type='string'></setattribute>
			<setattribute tag='MaximumHeight' RAttribute='MaximumHeight' enclosure='carrier' type='string'></setattribute>
